{
	"servers": {
		"plate": {
			"command": "bunx",
			"args": [
				"-y",
				"shadcn@canary",
				"registry:mcp"
			],
			"env": {
				"REGISTRY_URL": "https://platejs.org/r/registry.json"
			},
			"type": "stdio"
		},
		"playwright": {
			"command": "bunx",
			"args": [
				"@playwright/mcp@latest"
			],
		},
		"convex": {
			"command": "bunx",
			"args": [
				"convex@latest",
				"mcp",
				"start"
			],
		},
		"lucide-icons": {
			"command": "bunx",
			"args": [
				"lucide-icons-mcp@latest",
				"--stdio"
			]
		}
	}
}